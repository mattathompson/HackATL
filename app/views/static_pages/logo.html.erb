<div class="row">
  <div class="signin_box col-md-4 col-md-offset-4 col-sm-8 col-sm-offset-2">
    <h1>Upload Logo</h1>
    <%= form_tag( save_logo_path, multipart: true) do %>
      <div class="form-group">
        <%= file_field_tag :logo, class: "form-control" %>
      </div>

      <%= submit_tag "Submit Image", class: "btn btn-large btn-primary" %>
    <% end %>
  </div>
</div>
